{
  "component": {
    "pricingCard": {
      "radius": {
        "$type": "dimension",
        "$value": "{primitive.radius.lg}",
        "$description": "Card radius"
      },
      "bg": {
        "$type": "color",
        "$value": "{semantic.surface.page.default}",
        "$description": "Card background"
      },
      "borderColor": {
        "$type": "color",
        "$value": "{semantic.border.default}",
        "$description": "Card border"
      },
      "shadow": {
        "$type": "shadow",
        "$value": "{semantic.shadow.sm}",
        "$description": "Card shadow"
      },
      "padding": {
