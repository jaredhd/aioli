{
  "component": {
    "thumbsRating": {
      "gap": {
        "$type": "dimension",
        "$value": "{primitive.spacing.2}",
        "$description": "Gap between buttons"
      },
      "button": {
        "size": {
          "$type": "dimension",
          "$value": "{primitive.spacing.8}",
          "$description": "Button size"
        },
        "radius": {
          "$type": "dimension",
          "$value": "{primitive.radius.md}",
          "$description": "Button border radius"
        },
        "bg": {
          "$type": "color",
          "$value": "transparent",
          "$description": "Default button background"
        },
        "color": {
          "$type": "color",
          "$value": "{semantic.text.muted}",
          "$description": "Default button icon color"
        },
        "hoverBg": {
          "$type": "color",
          "$value": "{semantic.surface.card.hover}",
          "$description": "Button hover background"
        },
        "fontSize": {
          "$type": "dimension",
          "$value": "{primitive.font.size.lg}",
          "$description": "Icon font size"
        }
      },
      "up": {
        "activeBg": {
          "$type": "color",
          "$value": "{semantic.color.success.subtle}",
          "$description": "Active thumbs-up background"
        },
        "activeColor": {
          "$type": "color",
          "$value": "{semantic.color.success.default}",
          "$description": "Active thumbs-up color"
        }
      },
      "down": {
        "activeBg": {
          "$type": "color",
          "$value": "{semantic.color.danger.subtle}",
          "$description": "Active thumbs-down background"
        },
        "activeColor": {
          "$type": "color",
          "$value": "{semantic.color.danger.default}",
          "$description": "Active thumbs-down color"
        }
      },
      "feedback": {
        "bg": {
          "$type": "color",
          "$value": "{semantic.surface.page.default}",
          "$description": "Feedback textarea background"
        },
        "border": {
          "$type": "color",
          "$value": "{semantic.border.default}",
          "$description": "Feedback textarea border"
        },
        "radius": {
          "$type": "dimension",
          "$value": "{primitive.radius.md}",
          "$description": "Feedback textarea border radius"
        },
        "fontSize": {
          "$type": "dimension",
          "$value": "{primitive.font.size.sm}",
          "$description": "Feedback text font size"
        },
        "color": {
          "$type": "color",
          "$value": "{semantic.text.default}",
          "$description": "Feedback text color"
        },
        "placeholder": {
          "$type": "color",
          "$value": "{semantic.text.muted}",
          "$description": "Feedback placeholder color"
        }
      }
    }
  }
}
