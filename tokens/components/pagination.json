{
  "component": {
    "pagination": {
      "gap": { "$value": "{primitive.spacing.1}", "$type": "dimension" },
      "item": {
        "size": { "$value": "36px", "$type": "dimension" },
        "radius": { "$value": "{primitive.radius.md}", "$type": "dimension" },
        "fontSize": { "$value": "{primitive.font.size.sm}", "$type": "dimension" },
        "fontWeight": { "$value": "{primitive.font.weight.medium}", "$type": "fontWeight" },
        "bg": {
          "default": { "$value": "transparent", "$type": "color" },
          "hover": { "$value": "{primitive.color.neutral.100}", "$type": "color" },
          "active": { "$value": "{semantic.color.primary.default}", "$type": "color" },
          "disabled": { "$value": "transparent", "$type": "color" }
        },
        "color": {
          "default": { "$value": "{semantic.text.default}", "$type": "color" },
          "hover": { "$value": "{semantic.text.default}", "$type": "color" },
          "active": { "$value": "{primitive.color.neutral.0}", "$type": "color" },
          "disabled": { "$value": "{semantic.text.disabled}", "$type": "color" }
        },
        "border": {
          "width": { "$value": "1px", "$type": "dimension" },
          "color": { "$value": "{semantic.border.default}", "$type": "color" }
        }
      },
      "ellipsis": {
        "color": { "$value": "{semantic.text.muted}", "$type": "color" }
      }
    }
  }
