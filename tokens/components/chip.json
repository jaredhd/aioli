{
  "component": {
    "chip": {
      "height": {
        "sm": { "$value": "24px", "$type": "dimension" },
        "md": { "$value": "32px", "$type": "dimension" },
        "lg": { "$value": "40px", "$type": "dimension" }
      },
      "paddingX": {
        "sm": { "$value": "{primitive.spacing.2}", "$type": "dimension" },
        "md": { "$value": "{primitive.spacing.3}", "$type": "dimension" },
        "lg": { "$value": "{primitive.spacing.4}", "$type": "dimension" }
      },
      "radius": { "$value": "{primitive.radius.full}", "$type": "dimension" },
      "fontSize": { "$value": "{primitive.font.size.sm}", "$type": "dimension" },
      "fontWeight": { "$value": "{primitive.font.weight.medium}", "$type": "fontWeight" },
      "gap": { "$value": "{primitive.spacing.1}", "$type": "dimension" },
      "variant": {
        "filled": {
          "bg": { "$value": "{primitive.color.neutral.100}", "$type": "color" },
          "color": { "$value": "{semantic.text.default}", "$type": "color" },
          "bgHover": { "$value": "{primitive.color.neutral.200}", "$type": "color" }
        },
        "outlined": {
          "bg": { "$value": "transparent", "$type": "color" },
          "color": { "$value": "{semantic.text.default}", "$type": "color" },
          "border": { "$value": "{semantic.border.default}", "$type": "color" },
          "bgHover": { "$value": "{primitive.color.neutral.50}", "$type": "color" }
        },
        "primary": {
          "bg": { "$value": "{semantic.color.primary.subtle}", "$type": "color" },
          "color": { "$value": "{semantic.color.primary.default}", "$type": "color" },
          "bgHover": { "$value": "{semantic.color.primary.muted}", "$type": "color" }
        },
        "success": {
          "bg": { "$value": "{semantic.color.success.subtle}", "$type": "color" },
          "color": { "$value": "{semantic.color.success.default}", "$type": "color" }
        },
        "danger": {
          "bg": { "$value": "{semantic.color.danger.subtle}", "$type": "color" },
          "color": { "$value": "{semantic.color.danger.default}", "$type": "color" }
        }
      },
      "closeButton": {
        "size": { "$value": "16px", "$type": "dimension" },
        "color": { "$value": "{semantic.text.muted}", "$type": "color" },
        "hoverColor": { "$value": "{semantic.text.default}", "$type": "color" },
        "radius": { "$value": "{primitive.radius.full}", "$type": "dimension" }
      },
      "disabled": {
        "opacity": { "$value": 0.5, "$type": "number" }
      }
    }
  }
}
